<script src="/socket.io/socket.io.js"></script>
<script src="./index.js"></script>
<script>
    const eliminar = (id) => {
        const data = { id: id };
        fetch(`/api/productos/${id}`, {
            method: 'DELETE',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify(data),
        })
            .then((response) => response.json())
            .then((data) => {
                location.reload();
            })
            .catch((error) => {
                console.error('Error:', error);
            });
    };

